docker {
    enabled = true
    temp = 'auto'
}

profiles {

    test {

        docker {
            enabled = true
            temp = 'auto'
        }

        params {
            help = null

            baseDirReport = "${PWD}/results/reports"
            baseDirData   = "${PWD}/results/data"
        }
    }
}

// Load genomes config files

includeConfig 'configs/genomes.config'
